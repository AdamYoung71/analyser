digraph control_flow_graph {
	node [shape=box]
	l1 [label="x = 1" xlabel=l1]
	l2 [label="*y = &x" xlabel=l2]
	l1 -> l2
	l3 [label="*z = *a1" xlabel=l3]
	l2 -> l3
	l4 [label="call(*a2)" xlabel=l4]
	l3 -> l4
	l5 [label="*a3" xlabel=l5]
	l6 [label="*a1 = *a4" xlabel=l6]
	l5 -> l6 [label=true]
	l4 -> l5
}
